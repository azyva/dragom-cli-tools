<config>
	<root-classification-node>
		<properties>
		
			<!-- Git (GitScmPluginImpl). -->

			<!-- GIT_REPOS_BASE_URL provided as initialization property. -->
			<GIT_REPOS_SUFFIX>.git</GIT_REPOS_SUFFIX>

			<!-- SimpleArtifactInfoPluginImpl. -->

			<BASE_GROUP_ID>com.acme</BASE_GROUP_ID>

			<!-- ArtifactVersion to Version mappings (ConfigArtifactVersionMapperPluginImpl). -->
			
			<ARTIFACT_VERSION_TO_VERSION_MAPPINGS>PHASE_STATIC,PHASE_DYNAMIC,DYNAMIC,STATIC</ARTIFACT_VERSION_TO_VERSION_MAPPINGS>

			<ARTIFACT_VERSION_TO_VERSION_MAPPING_PHASE_STATIC>develop-(.+)-sprint(\d+)-SNAPSHOT:S/develop-$1-sprint$2</ARTIFACT_VERSION_TO_VERSION_MAPPING_PHASE_STATIC>
			<ARTIFACT_VERSION_TO_VERSION_MAPPING_TEST_EXISTENCE_PHASE_STATIC>true</ARTIFACT_VERSION_TO_VERSION_MAPPING_TEST_EXISTENCE_PHASE_STATIC>=

			<ARTIFACT_VERSION_TO_VERSION_MAPPING_PHASE_DYNAMIC>develop-(.+)-sprint(\d+)-SNAPSHOT:D/develop-$1-sprint</ARTIFACT_VERSION_TO_VERSION_MAPPING_PHASE_DYNAMIC>

			<ARTIFACT_VERSION_TO_VERSION_MAPPING_DYNAMIC>(.+)-SNAPSHOT:D/$1</ARTIFACT_VERSION_TO_VERSION_MAPPING_DYNAMIC>
			<ARTIFACT_VERSION_TO_VERSION_MAPPING_STATIC>(.+):S/v-$1</ARTIFACT_VERSION_TO_VERSION_MAPPING_STATIC>

			<!-- Version to ArtifactVersion mappings (ConfigArtifactVersionMapperPluginImpl). -->
			
			<VERSION_TO_ARTIFACT_VERSION_MAPPINGS>PHASE_DYNAMIC,PHASE_STATIC,DYNAMIC,STATIC</VERSION_TO_ARTIFACT_VERSION_MAPPINGS>

			<VERSION_TO_ARTIFACT_VERSION_MAPPING_PHASE_DYNAMIC>D/develop-(.+)-sprint:develop-$1-@PHASE-SNAPSHOT</VERSION_TO_ARTIFACT_VERSION_MAPPING_PHASE_DYNAMIC>
			<VERSION_TO_ARTIFACT_VERSION_MAPPING_ADD_PHASE_PHASE_DYNAMIC>true</VERSION_TO_ARTIFACT_VERSION_MAPPING_ADD_PHASE_PHASE_DYNAMIC>

			<VERSION_TO_ARTIFACT_VERSION_MAPPING_PHASE_STATIC>S/develop-(.+)-sprint(\d+):develop-$1-sprint$2-SNAPSHOT</VERSION_TO_ARTIFACT_VERSION_MAPPING_PHASE_STATIC>

			<VERSION_TO_ARTIFACT_VERSION_MAPPING_DYNAMIC>D/(.+):$1-SNAPSHOT</VERSION_TO_ARTIFACT_VERSION_MAPPING_DYNAMIC>
			<VERSION_TO_ARTIFACT_VERSION_MAPPING_STATIC>S/v-(.+):$1</VERSION_TO_ARTIFACT_VERSION_MAPPING_STATIC>

			<!-- UniformNewStaticVersionPluginImpl -->
			
			<REVISION_DECIMAL_POSITION_COUNT>1</REVISION_DECIMAL_POSITION_COUNT>


			<!-- SemanticNewStaticVersionPluginImpl -->

			<SEMANTIC_VERSION_PREFIX>v-</SEMANTIC_VERSION_PREFIX>

		</properties>
		<child-nodes>
			<classification-node>
				<name>Domain1</name>
				<properties>
					<GIT_REPOS_DOMAIN_FOLDER>Domain1</GIT_REPOS_DOMAIN_FOLDER>
				</properties>
			</classification-node>
			<classification-node>
				<name>Domain2</name>
				<properties>
					<GIT_REPOS_DOMAIN_FOLDER>Domain2</GIT_REPOS_DOMAIN_FOLDER>
				</properties>
			</classification-node>
			<classification-node>
				<name>Framework</name>
				<properties>
					<GIT_REPOS_DOMAIN_FOLDER>Framework</GIT_REPOS_DOMAIN_FOLDER>
				</properties>
			</classification-node>
		</child-nodes>
		<plugins>
			<plugin>
				<plugin-class>org.azyva.dragom.model.plugin.impl.UndefinedDescendantNodeManagerSimpleDynamicModulePluginImpl</plugin-class>
			</plugin>
			<plugin>
				<plugin-class>org.azyva.dragom.model.plugin.impl.GitScmPluginImpl</plugin-class>
			</plugin>
			<plugin>
				<plugin-class>org.azyva.dragom.model.plugin.impl.MavenReferenceManagerPluginImpl</plugin-class>
			</plugin>
			<plugin>
				<plugin-class>org.azyva.dragom.model.plugin.impl.SimpleArtifactInfoPluginImpl</plugin-class>
			</plugin>
			<plugin>
				<plugin-class>org.azyva.dragom.model.plugin.impl.SimpleFindModuleByArtifactGroupIdPluginImpl</plugin-class>
			</plugin>
			<plugin>
				<plugin-class>org.azyva.dragom.model.plugin.impl.ConfigArtifactVersionMapperPluginImpl</plugin-class>
			</plugin>
			<plugin>
				<plugin-class>org.azyva.dragom.model.plugin.impl.MavenBuilderPluginImpl</plugin-class>
			</plugin>
			<plugin>
				<plugin-class>org.azyva.dragom.model.plugin.impl.SimpleVersionClassifierPluginImpl</plugin-class>
			</plugin>

			<plugin>
				<plugin-interface>org.azyva.dragom.model.plugin.NewDynamicVersionPlugin</plugin-interface>
				<plugin-class>org.azyva.dragom.model.plugin.impl.RuntimeSelectionPluginFactory</plugin-class>
			</plugin>
			<plugin>
				<plugin-id>uniform</plugin-id>
				<plugin-class>org.azyva.dragom.model.plugin.impl.UniformNewDynamicVersionPluginImpl</plugin-class>
			</plugin>
			<plugin>
				<plugin-id>hotfix</plugin-id>
				<plugin-class>org.azyva.dragom.model.plugin.impl.HotfixNewDynamicVersionPluginImpl</plugin-class>
			</plugin>
			<plugin>
				<plugin-id>phase</plugin-id>
				<plugin-class>org.azyva.dragom.model.plugin.impl.PhaseNewDynamicVersionPluginImpl</plugin-class>
			</plugin>

			<plugin>
				<plugin-interface>org.azyva.dragom.model.plugin.NewStaticVersionPlugin</plugin-interface>
				<plugin-class>org.azyva.dragom.model.plugin.impl.RuntimeSelectionPluginFactory</plugin-class>
			</plugin>
			<plugin>
				<plugin-id>uniform</plugin-id>
				<plugin-class>org.azyva.dragom.model.plugin.impl.UniformNewStaticVersionPluginImpl</plugin-class>
			</plugin>
			<plugin>
				<plugin-id>semantic</plugin-id>
				<plugin-class>org.azyva.dragom.model.plugin.impl.SemanticNewStaticVersionPluginImpl</plugin-class>
			</plugin>
			<plugin>
				<plugin-id>phase</plugin-id>
				<plugin-class>org.azyva.dragom.model.plugin.impl.PhaseNewStaticVersionPluginImpl</plugin-class>
			</plugin>

			<plugin>
				<plugin-class>org.azyva.dragom.model.plugin.impl.MavenArtifactVersionManagerPluginImpl</plugin-class>
			</plugin>
<!--

			<plugin>
				<plugin-class>org.azyva.dragom.model.plugin.impl.NodeInitDemoNodeEventListenerPluginFactory</plugin-class>
				<ind-only-this-node>true</ind-only-this-node>
			</plugin>
-->
		</plugins>
	</root-classification-node>
</config>